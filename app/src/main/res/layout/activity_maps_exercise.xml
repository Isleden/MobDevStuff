<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/MapsMenu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MapsExercise">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/imgbtnStoNino"
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:src="@drawable/stonino_1"
            tools:src="@drawable/stonino_1"
            tools:srcCompat="@drawable/stonino_1" />

        <ImageButton
            android:id="@+id/imgbtnCebuOceanPark"
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:src="@drawable/oceanpark_1"
            tools:src="@drawable/oceanpark_1"
            tools:srcCompat="@drawable/oceanpark_1" />

        <ImageButton
            android:id="@+id/imgbtnFortSanPedro"
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:src="@drawable/fortspedro_1"
            tools:src="@drawable/fortspedro_1"
            tools:srcCompat="@drawable/fortspedro_1" />

        <ImageButton
            android:id="@+id/imgbtnTempleOfLeah"
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:src="@drawable/templeofleah_1"
            tools:src="@drawable/templeofleah_1"
            tools:srcCompat="@drawable/templeofleah_1" />

        <ImageButton
            android:id="@+id/imgbtnKawasanFalls"
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:src="@drawable/kawasanfalls1"
            tools:src="@drawable/kawasanfalls1"
            tools:srcCompat="@drawable/kawasanfalls1" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>